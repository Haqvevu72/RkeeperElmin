<Page x:Class="WpfApp1.View.Pages.AdminTable"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfApp1.View.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AdminTable">

    <Grid Background="Yellow">
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Height="100">
            <Button x:Name="AddFood" Style="{StaticResource button_2}" Click="AddFood_Click">Add Food To Menu</Button>
            <Button Name="Addtable" Style="{StaticResource button_2}" Click="AddTable_Click">Add Table</Button>
        </StackPanel>
    </Grid>
</Page>
